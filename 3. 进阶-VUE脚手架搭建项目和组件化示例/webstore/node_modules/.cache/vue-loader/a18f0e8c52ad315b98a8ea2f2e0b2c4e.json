4-划分模块</div>\n            <button @click=\"setMsg\">点我一下</button>\n        </div>\n\n    </div>\n</template>\n\n<script>\n    import {ADDITION, SUBSTRACTION} from '../store/MutationType';\n\n    export default {\n        name: 'Options',\n        data() {\n            return {}\n        },\n        methods: {\n\n            // 加法\n            add() {\n                this.$store.commit(ADDITION);\n            },\n\n            //减法\n            sub() {\n                this.$store.commit(SUBSTRACTION);\n            },\n            //\n            optionMore(num) {\n                this.$store.commit('optionMore', num);\n            },\n            //\n            setUser() {\n                this.$store.dispatch(\"setUserInfos\", 1).then(data => {\n                    console.log(data);\n                }).catch(error => {\n                    console.log(error);\n                });\n            },\n            setMsg() {\n                this.$store.commit(\"setMsg\", \"你好，我是战士！\");\n            }\n        }\n    }\n</script>\n<style scoped>\n    .box {\n        padding: 5px;\n        width: 200px;\n        box-shadow: 2px 2px 2px 2px #BABABA;\n    }\n    .box-item >div{\n        font-size: 15px;\n        font-weight: bold;\n        margin: 5px 3px;\n    }\n    .box-item >button{\n        margin: 0 5px;\n    }\n</style>"]}]}